datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Changelog {
  id        String  @id @default(uuid())
  repoOwner String
  project   String
  from      String  // ISO date: "2025-05-31"
  to        String  // ISO date: "2025-06-01"
  content   String
  createdAt DateTime @default(now())
}